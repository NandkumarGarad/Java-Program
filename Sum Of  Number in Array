import java.util.Scanner;
class SumOfNumber 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.print("Enter the array length : ");
		int size =s.nextInt();

		int[] a = new int[size];
		for (int i=0 ; i< a.length ; i++ )
		{
			a[i] = s.nextInt();
		}
		
		int sum = 0 ;
		for (int i=0 ; i<a.length ; i++)
		{
			sum += a[i];
		}

		System.out.println("Sum of array number is : "+sum);
	}
}

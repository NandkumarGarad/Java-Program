package strings;

public class DivisionAndModulus {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 62;
		int b = 3;
		int div =0;
		int prod = 0;
		for (int i = 0; i <b ; i++) {
			prod+=a;
		}
		System.out.println(a+" Multiplication "+b+" is : "+ prod);
		for (int i = 0; a>=b  ; i-=b) {
			 a-=b;
			 div++;
		}
		System.out.println(a+" Divided " + b +" is : "+ div);
		System.out.println(a+" Modulus "+b+ " is : "+a);
	}

}

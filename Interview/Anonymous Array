package strings;

public class AnaoymousArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int[] arr = new int[] {1,2,3};
		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i]);
		}
		
		// array createting two way 
		int [] a = new int[4];
		int [] b = {1,2,3,4,5,6,7};
		
		
		
		//int[] d = new int[3] {1,2,3};    // only one give the value either size or direct value of an anaoymous array.
		
		//array store in heap area(memory) in java. and reference store in stack memory.
	}

}

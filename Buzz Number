import java.util.Scanner;
class BuzzNumber 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);

		System.out.print("Enter the Number : ");
		int num = s.nextInt();

		if (num%7==0 || num%10==7)
		{
			System.out.println("It is Buzz Number.");
		}
		else
		{
			System.out.println("It is Not Buzz Number.");
		}

	}
}

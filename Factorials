import java.util.Scanner;
class Factorials 
{
	public static void main(String[] args) 
	{
		Scanner S = new Scanner(System.in);
		System.out.print("Enter the number : ");
		int num = S.nextInt();
		int fact = 1;
		for (int i = num;i > 0 ;i-- )
		{
			fact =fact * i;
		}

		System.out.println(fact);
	}
}

import java.util.Scanner;
class SecondMax 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner (System.in);
		System.out.print("Enter the first number : ");
		int a = s.nextInt();
		System.out.print("enter the second number : ");
		int b = s.nextInt();
		System.out.print("enter the third number : ");
		int c = s.nextInt();

		if (a>b)
		{
			if (a>c)
			{
				if (b>c)
				{
					System.out.println("Second Maximum number is : "+ b);
				}else
				{
					System.out.println("Second Maximum number is : "+c);
				}

			}else{
				System.out.println("Second Maximum number is : "+a);
			}
		}else
		{
			if (b>c)
			{
				System.out.println("Second Maximum number is : "+c);
			}else
			{
				System.out.println("Second Maximum number is : "+b);
			}
		}
	}
}

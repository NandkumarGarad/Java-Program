import java.util.Scanner;
class EvenNumber 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.print("Enter the array length : ");
		int size = s.nextInt();

		int[] a1 = new int[size];

		for (int i=0 ; i< a1.length ; i++ )
		{
			a1[i] = s.nextInt();
		}


		for (int i=0 ; i< a1.length ; i++ )
		{
			if (a1[i]%2==0)
			{
				System.out.print(a1[i]+" ");
			}
		}

		
	}
}

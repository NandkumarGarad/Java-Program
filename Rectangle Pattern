import java.util.Scanner;
class Ractangle2 
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);

		System.out.print("Enter the Number : ");
		int N = s.nextInt();

		for (int i=0 ; i<N ; i++ )
		{
			for (int j=0 ; j<N ; j++ )
			{
				if( i==0 || i==j || i==N-1 || j==0 || i+j==N-1 || j==N-1)
				{
					System.out.print("*"+" ");
				}
				else
				{
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}
}
